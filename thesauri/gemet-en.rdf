      <skos:scopeNote xml:lang="en">The process of applying fire or heat, as in the hardening or glazing of ceramics.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/12605"/>
      <skos:related rdf:resource="http://www.eionet.europa.eu/gemet/concept/930"/>
