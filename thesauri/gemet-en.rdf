   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/3238">
      <skos:prefLabel xml:lang="en">fisheries management</skos:prefLabel>
      <skos:scopeNote xml:lang="en">The administration and handling of aspects of the fishing industry, including the catching, processing and selling of fish.</skos:scopeNote>
      <skos:related rdf:resource="http://www.eionet.europa.eu/gemet/concept/3237"/>
