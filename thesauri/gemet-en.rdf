   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/7630">
      <skos:prefLabel xml:lang="en">sewage farm</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Area of land on which sewage or any other type of waste water is distributed in order to purify it; it is a kind of waste water treatment.</skos:scopeNote>
