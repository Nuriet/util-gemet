      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/10019"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/10058">
      <skos:prefLabel xml:lang="en">surface water management</skos:prefLabel>
      <skos:scopeNote xml:lang="en">The administration or handling of water naturally open to the atmosphere (rivers, lakes, reservoirs, ponds, streams, seas, etc.).</skos:scopeNote>
