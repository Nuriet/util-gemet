   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/3012">
      <skos:prefLabel xml:lang="en">evaluation criterion</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A standard, norm, value or measurement by which the quantity or quality of a process, object or person's work performance is ascertained through an analysis and judgment of the relevant information in context and in view of established goals, objectives and standards.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/6373"/>
   </skos:Concept>
