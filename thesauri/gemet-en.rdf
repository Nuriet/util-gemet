      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/1023"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/538">
      <skos:prefLabel xml:lang="en">architecture</skos:prefLabel>
      <skos:scopeNote xml:lang="en">The art and science of designing and building structures, or large groups of structures, in keeping with aesthetic and functional criteria.</skos:scopeNote>
