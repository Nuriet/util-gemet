   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/7681">
      <skos:prefLabel xml:lang="en">refuse shredder</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A machine used to break up refuse material into smaller pieces by tearing and/or impact.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/9051"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/7683">
      <skos:prefLabel xml:lang="en">shrub</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A woody perennial plant, smaller than a tree, with several major branches arising from near the base of the main stem.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/1020"/>
