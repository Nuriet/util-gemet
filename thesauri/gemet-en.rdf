   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/4360">
      <skos:prefLabel xml:lang="en">inspection</skos:prefLabel>
      <skos:scopeNote xml:lang="en">An official examination and evaluation of the extent to which specified goals, objectives, standards, policies or procedures of an agency, organization, department or unit have been met properly.</skos:scopeNote>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/4361"/>
