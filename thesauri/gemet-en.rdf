      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/6228"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/5150">
      <skos:prefLabel xml:lang="en">membrane</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A thin tissue that encloses or lines biological cells, organs, or other structures. It consists of a double layer of lipids with protein molecules between the two layers. Membranes are permeable to water and fat-soluble substances but not to such polar molecules as sugars.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/8494"/>
