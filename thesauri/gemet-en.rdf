   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/2736">
      <skos:prefLabel xml:lang="en">energy recovery</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A form of resource recovery in which the organic fraction of waste is converted to some form of usable energy. Recovery may be achieved through the combustion of processed or raw refuse to produce steam through the pyrolysis of refuse to produce oil or gas; and through the anaerobic digestion of organic wastes to produce methane gas.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/2749"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/2738">
      <skos:prefLabel xml:lang="en">energy resource</skos:prefLabel>
