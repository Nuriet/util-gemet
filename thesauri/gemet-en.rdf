      <skos:scopeNote xml:lang="en">A mine where all the mineral that could be profitably exploited has been removed.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/3032"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/9369">
      <skos:prefLabel xml:lang="en">working condition</skos:prefLabel>
      <skos:scopeNote xml:lang="en">All existing circumstances affecting labor in the workplace, including job hours, physical aspects, legal rights and responsibilities.</skos:scopeNote>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/4015"/>
