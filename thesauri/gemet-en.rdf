   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/4309">
      <skos:prefLabel xml:lang="en">information service</skos:prefLabel>
      <skos:scopeNote xml:lang="en">An organized system of providing assistance or aid to individuals who are seeking information, such as by using databases and other information sources to communicate or supply knowledge or factual data.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/1605"/>
