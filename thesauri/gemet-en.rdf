      <skos:prefLabel xml:lang="en">fishery resource</skos:prefLabel>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/3273"/>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/7525"/>
