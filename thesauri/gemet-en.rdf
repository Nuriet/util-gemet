      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/3688"/>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/3757"/>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/8892"/>
