   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/7632">
      <skos:prefLabel xml:lang="en">sewage sludge</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A semi-liquid waste with a solid concentration in excess of 2500 parts per million, obtained from the purification of municipal sewage.</skos:scopeNote>
