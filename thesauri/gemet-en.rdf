   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/4331">
      <skos:prefLabel xml:lang="en">inland navigation</skos:prefLabel>
      <skos:scopeNote xml:lang="en">The navigation of inland waterways, i.e. navigable rivers, canals, sounds, lakes, inlets, etc.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/11289"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/4333">
      <skos:prefLabel xml:lang="en">inland water</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A lake, river, or other body of water wholly within the boundaries of a state.</skos:scopeNote>
