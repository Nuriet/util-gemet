   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/3853">
      <skos:prefLabel xml:lang="en">hazard of pollutants</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Risk or danger to human health, property or the environment posed by the introduction of a harmful substances into the ecosystem.</skos:scopeNote>
