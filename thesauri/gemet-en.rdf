   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/103">
      <skos:prefLabel xml:lang="en">administrative competence</skos:prefLabel>
      <skos:scopeNote xml:lang="en">The skill, knowledge, qualification, capacity or authority to manage or direct the affairs of a public or private office, business or organization.</skos:scopeNote>
