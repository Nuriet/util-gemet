   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/736">
      <skos:prefLabel xml:lang="en">batch process</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A process that is not in continuous or mass production; operations are carried out with discrete quantities of material or a limited number of items.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/4257"/>
