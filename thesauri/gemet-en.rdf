   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/8076">
      <skos:prefLabel xml:lang="en">statistics</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A branch of mathematics dealing with the collection, analysis, interpretation, and presentation of masses of numerical data.</skos:scopeNote>
