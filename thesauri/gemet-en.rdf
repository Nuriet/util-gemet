      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/12586"/>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/7472"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/4964">
