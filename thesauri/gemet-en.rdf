   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/3854">
      <skos:prefLabel xml:lang="en">hazardous substance</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Any material that poses a threat to human health and/or the environment. Typical hazardous substances are toxic, corrosive, ignitable, explosive, or chemically reactive.</skos:scopeNote>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/2869"/>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/3859"/>
