   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/1473">
      <skos:prefLabel xml:lang="en">climatic experiment</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Experiments conducted to estimate future climatic conditions employing modelling of the physical processes underlying climatic change and variability; also, assessments are required of uncertain future man-made inputs such as increasing atmospheric carbon dioxide and other green-house gases.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/7702"/>
      <skos:related rdf:resource="http://www.eionet.europa.eu/gemet/concept/1462"/>
   </skos:Concept>
