   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/2651">
      <skos:prefLabel xml:lang="en">emancipation</skos:prefLabel>
      <skos:scopeNote xml:lang="en">The state of being free from social or political restraint or from the inhibition of moral or social conventions.</skos:scopeNote>
