   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/8072">
      <skos:prefLabel xml:lang="en">state of matter</skos:prefLabel>
      <skos:scopeNote xml:lang="en">One of the three fundamental conditions of matter: the solid, the liquid, and gaseous states.</skos:scopeNote>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/157"/>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/1568"/>
