      <skos:scopeNote xml:lang="en">A plastic, paper, or metal tape that is coated or impregnated with magnetizable iron oxide particles, used in magnetic recording.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/1981"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/4965">
      <skos:prefLabel xml:lang="en">magnetism</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A class of physical phenomena associated with moving electricity, including the mutual mechanical forces among magnets and electric currents.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/6229"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/4967">
