      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/8574"/>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/13278"/>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/8571"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/8574">
