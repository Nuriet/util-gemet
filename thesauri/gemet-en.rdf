   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/8077">
      <skos:prefLabel xml:lang="en">waste statistics</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Determination of the quantity and character of the wastes discarded by a community, by spot sampling procedure.</skos:scopeNote>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/9141"/>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/8076"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/8078">
      <skos:prefLabel xml:lang="en">status of development</skos:prefLabel>
      <skos:scopeNote xml:lang="en">The extent to which a society promotes human well-being in all dimensions of existence by forming people's capabilities, expanding choices and increasing opportunities.</skos:scopeNote>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/2147"/>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/2152"/>
