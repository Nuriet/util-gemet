      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/9269"/>
      <skos:related rdf:resource="http://www.eionet.europa.eu/gemet/concept/11"/>
      <skos:related rdf:resource="http://www.eionet.europa.eu/gemet/concept/1464"/>
      <skos:related rdf:resource="http://www.eionet.europa.eu/gemet/concept/1473"/>
      <skos:related rdf:resource="http://www.eionet.europa.eu/gemet/concept/1476"/>
      <skos:related rdf:resource="http://www.eionet.europa.eu/gemet/concept/11133"/>
