      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/6527"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/4985">
      <skos:prefLabel xml:lang="en">management</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Government, control, superintendence, physical or manual handling or guidance; act of managing by direction or regulation, or administration, as management of family, or of household, etc.</skos:scopeNote>
