   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/3859">
      <skos:prefLabel xml:lang="en">hazardous working material</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A poison, corrosive agent, flammable substance, explosive, radioactive chemical, or any other material which can endanger human health or well-being if handled improperly.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/3854"/>
