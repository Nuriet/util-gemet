   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/3894">
      <skos:prefLabel xml:lang="en">heating</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A system for supplying heat, especially central heating, to a building.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/12606"/>
      <skos:related rdf:resource="http://www.eionet.europa.eu/gemet/concept/3892"/>
