      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/6225"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/865">
      <skos:prefLabel xml:lang="en">biological pollutant</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Viruses, bacteria, fungi, and mammal and bird antigens that may be present in the environment and cause many health effects.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/6395"/>
