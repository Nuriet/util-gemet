   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/2890">
      <skos:prefLabel xml:lang="en">environmental plan</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A formulated or systematic method for the protection of natural or ecological resources.</skos:scopeNote>
