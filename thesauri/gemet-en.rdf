      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/10003"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/5163">
      <skos:prefLabel xml:lang="en">metabolism of pesticides</skos:prefLabel>
      <skos:scopeNote xml:lang="en">The sum of chemical reactions, including both synthesis and breakdown, that occurs in substances or mixtures intended to prevent, destroy or mitigate pests that are directly or indirectly detrimental to harvest crops and other humans interests.</skos:scopeNote>
