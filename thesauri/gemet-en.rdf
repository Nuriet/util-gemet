   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/437">
      <skos:prefLabel xml:lang="en">animal physiology</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Study of the normal processes and metabolic functions of animal organisms.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/6240"/>
