   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/7796">
      <skos:prefLabel xml:lang="en">social group</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A collection of people who interact with one another and share a certain feeling of unity.</skos:scopeNote>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/607"/>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/1738"/>
