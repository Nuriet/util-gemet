   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/8051">
      <skos:prefLabel xml:lang="en">standard for building industry</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A norm or measure applicable in legal cases for any enterprise involved in the construction, remodeling or finishing of enclosed structures for habitation.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/7367"/>
