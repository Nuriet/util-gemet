   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/8663">
      <skos:prefLabel xml:lang="en">treaty</skos:prefLabel>
      <skos:scopeNote xml:lang="en">An international agreement in writing between two states or a number of states. Treaties are binding in international law; some treaties create law only for those states that are parties to them.</skos:scopeNote>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/13279"/>
