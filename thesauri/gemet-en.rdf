      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/7919"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/8073">
      <skos:prefLabel xml:lang="en">station</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A place along a route or line at which a bus, train, etc. stops for fuel or to pick up or let off passengers or goods, especially with ancillary buildings and services.</skos:scopeNote>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/11269"/>
