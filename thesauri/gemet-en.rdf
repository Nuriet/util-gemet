   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/8067">
      <skos:prefLabel xml:lang="en">state of the art</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Everything made available to the public by means of a written or oral description, by use or in any other way before the date of the patent application, or an application filed in a foreign country the priority of which is validly claimed.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/2775"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/8068">
