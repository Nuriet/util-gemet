      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/13207"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/11157">
      <skos:prefLabel xml:lang="en">agricultural disaster</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Violent, sudden and destructive change in the environment either affecting or caused by land cultivation or the raising of crops or livestock.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/4052"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/11158">
      <skos:prefLabel xml:lang="en">agricultural undervaluation</skos:prefLabel>
      <skos:scopeNote xml:lang="en">The underrating or diminishing in value of agricultural or farming goods and services.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/11234"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/11159">
      <skos:prefLabel xml:lang="en">rural development</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Any course destined to promote economic growth, modernization, increase in agricultural production and the creation of a framework in which to fulfill primary needs, such as education, health and supply of water in the rural areas. The attainment of such objectives depends in general on the type of administrative systems proposed for the various programmes and on the national political situation as regards, for instance land tenure, agrarian reform, the disbursement of assistance and food policy.</skos:scopeNote>
      <skos:related rdf:resource="http://www.eionet.europa.eu/gemet/concept/8804"/>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/213"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/11165">
      <skos:prefLabel xml:lang="en">agricultural hydraulics</skos:prefLabel>
