      <skos:scopeNote xml:lang="en">Any series of procedures and devices used by trained personnel to provide immediate assistance to persons who are in danger or injured.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/10054"/>
      <skos:related rdf:resource="http://www.eionet.europa.eu/gemet/concept/3226"/>
