   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/5913">
      <skos:prefLabel xml:lang="en">organic waste</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Waste containing carbon compounds; derived from animal and plant materials.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/1651"/>
