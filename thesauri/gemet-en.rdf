   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/3572">
      <skos:prefLabel xml:lang="en">gaseous air pollutant</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Uncondensed or volatile gases, usually comprised of chemical compounds, discharged to the atmosphere.</skos:scopeNote>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/4625"/>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/9042"/>
