   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/4307">
      <skos:prefLabel xml:lang="en">information processing</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A systematic series of actions performed by a person or computer on data elements including classifying, sorting, calculating, summarizing, transmitting, retrieving and receiving.</skos:scopeNote>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/5167"/>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/4303"/>
