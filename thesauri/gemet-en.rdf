   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/8057">
      <skos:prefLabel xml:lang="en">starch</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A polysaccharide which is a combination of many monosaccharide molecules, made during photosynthesis and stored as starch grains in many plants.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/1159"/>
