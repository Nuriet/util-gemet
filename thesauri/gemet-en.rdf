      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/4046"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/4979">
      <skos:prefLabel xml:lang="en">malnutrition</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Defective nutrition due to inadequate intake of nutrients or to their faulty digestion, assimilation or metabolism.</skos:scopeNote>
