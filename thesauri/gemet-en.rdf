   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/4310">
      <skos:prefLabel xml:lang="en">information source</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Generally, any resource initiating and substantiating the reception of knowledge or specifically, the origin of a data transmission.</skos:scopeNote>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/14892"/>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/1605"/>
