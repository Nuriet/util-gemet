   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/4356">
      <skos:prefLabel xml:lang="en">insectivore</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Any placental mammal of the order Insectivora, being typically small, with simple teeth, and feeding on invertebrates. The group includes shrews, moles, and hedgehogs.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/4982"/>
