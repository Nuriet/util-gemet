      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/8769"/>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/8946"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/4984">
