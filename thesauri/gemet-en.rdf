   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/5390">
      <skos:prefLabel xml:lang="en">motor vehicle</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A road vehicle driven by a motor or engine, especially an internal-combustion engine.</skos:scopeNote>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/1148"/>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/4913"/>
