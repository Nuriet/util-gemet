   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/10847">
      <skos:prefLabel xml:lang="en">leisure centre</skos:prefLabel>
      <skos:scopeNote xml:lang="en">A building containing a swimming pool and a large room or other places where you can play sports.</skos:scopeNote>
      <skos:narrower rdf:resource="http://www.eionet.europa.eu/gemet/concept/7668"/>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/4321"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/10851">
