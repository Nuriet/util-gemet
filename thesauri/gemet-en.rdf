   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/8659">
      <skos:prefLabel xml:lang="en">travel cost</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Expenditure of money or the amount of money incurred for journeying or going from one place to another by some mode of transportation.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/1824"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/866">
      <skos:prefLabel xml:lang="en">biological pollution</skos:prefLabel>
      <skos:scopeNote xml:lang="en">Disturbance of the ecological balance by the accidental or deliberate introduction of a foreign organism, animal or plant species into an environment.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/10048"/>
      <skos:related rdf:resource="http://www.eionet.europa.eu/gemet/concept/865"/>
