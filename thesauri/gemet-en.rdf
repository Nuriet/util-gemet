      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/496"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/11170">
      <skos:prefLabel xml:lang="en">farming technique</skos:prefLabel>
      <skos:scopeNote xml:lang="en">The business, art, or skill of agriculture.</skos:scopeNote>
