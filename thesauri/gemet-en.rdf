   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/3550">
      <skos:prefLabel xml:lang="en">galvanisation</skos:prefLabel>
      <skos:scopeNote xml:lang="en">The act of coating iron or steel with zinc, either by immersion in a bath of molten zinc or by deposition from a solution of zinc sulphate, to give protection against corrosion.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/12605"/>
