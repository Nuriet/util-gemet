      <skos:scopeNote xml:lang="en">Partly finished goods or products that re-enter into production elsewhere.</skos:scopeNote>
      <skos:broader rdf:resource="http://www.eionet.europa.eu/gemet/concept/10930"/>
   </skos:Concept>
   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/4412">
