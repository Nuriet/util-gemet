   <skos:Concept rdf:about="http://www.eionet.europa.eu/gemet/concept/13154">
      <skos:prefLabel xml:lang="en">social framework</skos:prefLabel>
      <skos:scopeNote xml:lang="en">The underlying structure that connects and supports the various members and parts of a community or human organization.</skos:scopeNote>
